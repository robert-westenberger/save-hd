{
  "brace-style": [2, "1tbs", { "allowSingleLine": true }],
  "class-methods-use-this": 0,
  "curly": [2, "all"],
  "import/extensions": 1,
  "import/no-extraneous-dependencies": [2, { "devDependencies": false, "optionalDependencies": false, "peerDependencies": false }],
  "import/prefer-default-export": 0,
  "linebreak-style": "off",
  "max-len": ["error", { "code": 200, "ignoreComments": true, "ignoreTrailingComments": true }],
  "no-case-declarations": 0,
  "no-cond-assign": 2,
  "no-console": [2, { "allow": ["warn", "error"] }],
  "no-continue": 0,
  "no-lone-blocks": 2,
  "no-param-reassign": [2, { "props": false }],
  "no-undef": 2,
  "no-underscore-dangle": 0,
  "no-unused-expressions": [2, { "allowShortCircuit": true }],
  "padded-blocks": 0,
  "prefer-destructuring": [2, { "array": false, "object": true }, {"enforceForRenamedProperties": false}],
  "prefer-spread": 2,
  "require-jsdoc": 0,
  "valid-jsdoc": 0,
  "sort-vars": 2,
  "quote-props": 0,
  "arrow-parens": 0,
  "camelcase": ["error", {
    "allow": ["_(.*)"],
    "properties": "never"
  }],
  "comma-dangle": 0,
  "comma-style": 0,
  "compat/compat": 0,
  "function-paren-newline": 0,
  "implicit-arrow-linebreak": 0,
  "import/default": 2,
  "import/named": 2,
  "import/namespace": 2,
  "import/no-cycle": 0,
  "import/no-named-as-default": 2,
  "import/no-named-as-default-member": 2,
  "import/no-unresolved": 2,
  "import/no-useless-path-segments": 2,
  "indent": 0,
  "key-spacing": 0,
  "lines-between-class-members": 0,
  "max-classes-per-file": 0,
  "no-multi-spaces": 0,
  "no-restricted-globals": 0,
  "no-restricted-syntax": 0,
  "no-invalid-this": 0,
  "object-curly-newline": 0,
  "operator-assignment": 0,
  "operator-linebreak": 0,
  "prefer-object-spread": 0,
  "semi-style": 0,
  "spaced-comment": 0,
  "switch-colon-spacing": 0
}